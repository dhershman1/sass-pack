  },
  "bugs": {
    "url": "https://bitbucket.org/dhershman/sasspack/issues"
  },
  "author": "Dustin Hershman",
  "license": "ISC",
  "bin": {
    "sass-pack": "./index.js"
  },
  "devDependencies": {
    "rimraf": "^2.5.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "node-sass": "^3.10.0",
    "fs-promise": "^1.0.0",
    "globby": "^6.1.0",
    "minimist": "^1.2.0"
  }
