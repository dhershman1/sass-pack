{
  "name": "sass-pack",
  "version": "1.3.0",
  "description": "Compile Sass down for a cssmanifest",
  "scripts": {
    "clean": "rimraf tests/outputs/*.{css,json}",
    "test": "npm run clean && node tests/tests.js | tap-spec",
